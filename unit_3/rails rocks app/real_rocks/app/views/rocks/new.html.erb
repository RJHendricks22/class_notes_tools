<%= render 'header' %>
<%= render 'home' %>
<hr>
<form action="/rocks" method="post">
  <h2>Add a new Rock: </h2>
    <br>
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <label>Rock Name: <input name='name' placeholder="Rock Name" type="text"><br></label>
    <label>Description: <input name='description' type="text" placeholder="Description"><br></label>
    <label>Image Link: <input name='image' type="text" placeholder="Image Link"><br></label>
    <button>Submit</button>
  </form>

